{
  "Cors": {
    "AllowedOrigins": [
      "https://interactive-board-front.vercel.app/",
      "https://interactive-board-front.vercel.app"
    ]
  },
  "Jwt": {
    "Issuer": "{ISSUER}",
    "Audience": "{AUDIENCE}",
    "SecurityKey": "{SECURITY_KEY}",
    "ExpireInMinutes": "5",
    "ClientId": "{CLIENT_ID}",
    "ClientSecret": "{CLIENT_SECRET}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
